<%- include('../sidebar.ejs') %>
    <%- include('../header.ejs') %>


    <div class="content mt-3">
        <div class="animated fadeIn">
            <div class="row">
                <div class="col-md-12">
                    <div class="card">
                        <div class="card-header d-flex justify-content-between">
                            <div>
                              <strong class="card-title">Contact list</strong>
                            </div>
                        </div>
                        <div class="card-body">
                            <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                                <thead class="bg-success">
                                    <tr>
                                        <th>id</th>
                                        <th>name</th>
                                        <th>email</th>
                                        <th>mobile_number</th>
                                        <th>subject</th>
                                        <th>message</th>
                                        <th>Action</th>
                                    </tr>
            
                                </thead>
            
                                <tbody>
                                    <% i=1 %>
                                        <% c.forEach((item)=>{ %>
            
                                            <tr>
                                                <td>
                                                    <%= i++ %>
                                                </td>
                                                <td>
                                                    <%= item.name %>
                                                </td>
                                                <td>
                                                    <%= item.email %>
                                                </td>
                                                <td>
                                                    <%= item.mobile_number %>
                                                </td>
                                                <td>
                                                    <%= item.subject %>
                                                </td>
                                                <td>
                                                    <%= item.message %>
                                                </td>
            
                                                <td>
                                                    <a href="/admin/contactview/<%= item._id %>" class="btn btn-info">View</a>
                                                    <a href="/admin/contactdelete/<%= item._id %>" class="btn btn-danger">Delete</a>
                                                </td>
            
                                            </tr>
            
                                            <% }) %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </div><!-- .animated -->
      
      
      </div><!-- .content -->
      </div><!-- /#right-panel -->




        <%- include('../footer.ejs') %>