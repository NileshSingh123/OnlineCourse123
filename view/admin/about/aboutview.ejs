    <div class="animated fadeIn">
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header d-flex justify-content-between">
                        <div>
                          <strong class="card-title">View</strong>
                        </div>
                        
                    </div>
                    <div class="card-body">
                        <table id="bootstrap-data-table-export" class="table table-striped table-bordered">
                            <thead>
                                <tr>
                                  <th>id</th>
                                    <th>name</th>
                                    <th>post</th>
                                    <th>description</th>
                                    <th>image</th>
                                    
  
                                </tr>


                            </thead>
                            <tbody>
                                    <tr>
                                      <td><%= view._id %></td>
                                        <td><%= view.name %></td>
                                        <td><%= view.post %></td>
                                        <td><%= view.description %></td>
                                        <td><img src="<%= view.image.url %>" ></td>
                                    
                                    </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
  
  
  </div>
